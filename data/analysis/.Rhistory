data_direct <- read.csv(file="direct-distribution-moas.csv", head=TRUE, sep=",", quote = "\'")
summary(data_direct)
no_direct <- data_direct$numberOfOccurences
hist(no_direct, breaks=cat, ylab="Frequency", xlab="# of direct MoAs", main="Direct MoA distribution")
abline(v = mean(no_direct), col = "red", lwd = 1)
abline(v = median(no_direct), col = "blue", lwd = 1)
